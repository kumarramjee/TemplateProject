<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="riocatlog.mobimedia.com.templaterioproject.ui.ui.NextActivity">

    <ListView
        android:id="@+id/listeditem"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></ListView>

    <View
        android:id="@+id/listeditemview"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="#80d3d3d3" />

    <ListView
        android:id="@+id/prodctlist"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/listeditemview"></ListView>
</RelativeLayout>
